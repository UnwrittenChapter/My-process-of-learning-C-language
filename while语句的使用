#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int password[20] = { '0' };
	    printf("请输入您的密码：");
	scanf("%s", password);
	int tem = 0;
	while ((tem = getchar()) != '\n')
	{

		;
	}
	    printf("请确认您的密码(Y/N)");
	int ch = getchar();
	if (ch == 'Y')
	{
		printf("输入成功\n");

	}
	else
	{
		printf("输入失败\n");

	}
	return 0;
}
