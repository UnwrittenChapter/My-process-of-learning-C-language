include <stdio.h>
struct Student
{
	char name[20];
	int age;
	double score;
};
int main()
{
	struct Student s = { "张三",20,90.0 };
	printf("1: %s %d %lf\n", s.name, s.age, s.score);
	struct Student* ps = &s;
	printf("2: %s %d %lf\n", (*ps).name , (*ps).age, (*ps).score);
	printf("3: %s %d %lf\n", ps->name, ps->age, ps->score);
	return 0;
}
